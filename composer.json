{
    "name": "phlexible/redactor-bundle",
    "description": "phlexible redactor bundle",
    "license": "MIT",
	"type": "symfony-bundle",
	"keywords": ["redactor", "editor", "rich text"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\RedactorBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "phlexible/gui-bundle": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/config": "~2.8",
        "symfony/dependency-injection": "~2.8",
        "symfony/http-kernel": "~2.8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
